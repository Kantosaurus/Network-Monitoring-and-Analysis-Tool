{
  "name": "network-monitor-analysis-tool",
  "version": "1.0.0",
  "description": "Network monitoring and analysis tool similar to Wireshark",
  "main": "main.js",
  "bin": {
    "nmat-tshark": "./tshark-cli.js",
    "nmat": "./cli/nmat-cli.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "vite build && electron .",
    "start": "npm run electron:dev",
    "rebuild": "electron-rebuild",
    "postinstall": "electron-rebuild"
  },
  "keywords": [
    "network",
    "monitor",
    "packet",
    "capture",
    "analysis"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "electron": "^28.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.35.0",
    "@types/animejs": "^3.1.13",
    "animejs": "^3.2.2",
    "axios": "^1.6.2",
    "better-sqlite3": "^12.4.1",
    "body-parser": "^2.2.0",
    "cap": "^0.2.1",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.1.1",
    "commander": "^11.0.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "http-mitm-proxy": "^1.1.0",
    "http-proxy": "^1.18.1",
    "motion": "^12.23.24",
    "node-cron": "^3.0.3",
    "node-forge": "^1.3.1",
    "puppeteer": "^21.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0"
  }
}
